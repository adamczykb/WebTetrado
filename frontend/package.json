{
  "name": "webtetrado",
  "version": "1.0.0",
  "description": "ElTetrado web service",
  "main": "./src/index.tsx",
  "scripts": {
    "start": "HOST=0.0.0.0 concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"react-app-rewired start\"",
    "build": "./compile_less.sh && webpack --mode=production --node-env=production ",
    "less": "less-watch-compiler --config less-watcher.config.json",
    "lessc": "./compile_less.sh",
    "test": "react-app-rewired test",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"webpack serve\""
  },
  "author": "Bartosz Adamczyk",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "antd": "^4.18.5",
    "css-mediaquery": "^0.1.2",
    "molstar": "^3.4.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "testing-library": "^0.0.2",
    "uuidv4": "^6.2.12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "less-watch-compiler": "^1.16.3",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "sass": "^1.49.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.8.4",
    "web-vitals": "^3.0.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  }
